<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="846.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.BackTrackControl">
	<children>
		<TableView fx:id="tableview" editable="true" layoutX="766.0" layoutY="14.0" prefHeight="400.0" prefWidth="500.0">
			<columns>
				<TableColumn fx:id="itemCol" prefWidth="75.0" text="item" />
				<TableColumn fx:id="weightCol" prefWidth="75.0" text="weight" />
				<TableColumn fx:id="valueCol" prefWidth="75.0" text="value" />
				<TableColumn fx:id="ratioCol" prefWidth="75.0" text="ratio" />
			</columns>
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
		</TableView>
		<ListView fx:id="listview" editable="true" layoutX="548.0" layoutY="14.0" onMouseClicked="#listViewClicked" prefHeight="400.0" prefWidth="200.0" />
      <HBox alignment="CENTER" layoutX="766.0" layoutY="423.0" prefHeight="25.0" prefWidth="500.0" spacing="25.0">
         <children>
            <TextField fx:id="nameField" />
            <TextField fx:id="weightField" alignment="CENTER" prefHeight="25.0" prefWidth="106.0" />
            <TextField fx:id="valueField" alignment="CENTER" prefHeight="25.0" prefWidth="106.0" />
      		<Button fx:id="addStuffButton" mnemonicParsing="false" onAction="#addStuff" text="add stuff" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="548.0" layoutY="423.0" prefHeight="25.0" prefWidth="200.0" spacing="25.0">
         <children>
            <Button fx:id="newStuffButton" mnemonicParsing="false" onAction="#newStuff" text="new stuff" />
            <Button fx:id="saveStuffButton" mnemonicParsing="false" onAction="#saveStuff" text="save stuff" />
         </children>
      </HBox>
	</children>
</AnchorPane>
